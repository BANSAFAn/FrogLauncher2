<link rel="stylesheet" href="modals/packs.css"/>

<div class="modal" id="modal-packs" style="display: none;">
    <div class="flex flex-gap-8 flex-align-center">
        <span class="material-symbols-outlined">package_2</span>
        <h1>{{flymenu.packs}}</h1>
    </div>
    <div class="wrapper">
        <div class="grid">
            <div class="item add">
                <button class="button pill small install">
                    <span class="material-symbols-outlined">download</span>
                    <span>{{packManager.tabs.install}}</span>
                </button>
                <button class="button pill small create">
                    <span class="material-symbols-outlined">add</span>
                    <span>{{commons.create}}</span></button>
                <button class="button pill small import">
                    <span class="material-symbols-outlined">browser_updated</span>
                    <span>{{packManager.tabs.import}}</span>
                </button>
            </div>
        </div>
    </div>
</div>

<script src="modals/packs.js"></script>